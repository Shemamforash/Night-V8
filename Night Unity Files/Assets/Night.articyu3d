        {
          "Type": "Document",
          "Properties": {
            "TechnicalName": "Doc_8AE71A7B",
            "Id": "0x10000000000094A",
            "Parent": "0x3000000103AA2F0",
            "DisplayName": "Implementation",
            "Color": {
              "r": 0.5764706,
              "g": 0.75686276,
              "b": 0.8
            },
            "Text": "",
            "ExternalId": "",
            "Position": {
              "x": 140.0,
              "y": 140.0
            },
            "ZIndex": 7.0,
            "Size": {
              "w": 256.0,
